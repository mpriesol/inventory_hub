{
  "name": "Northfinder",
  "is_active": true,
  "feeds": {
    "current_key": "products",
    "sources": {
      "products": {
        "mode": "remote",
        "local_path": null,
        "remote": {
          "url": "https://b2b.northfinder.com/xml/partners/dispatcher.php?param=43&token=FILL_ME_NORTHFINDER_TOKEN",
          "method": "GET",
          "headers": {},
          "params": {},
          "auth": {
            "mode": "none"
          }
        }
      },
      "stock": {
        "mode": "remote",
        "local_path": null,
        "remote": {
          "url": "https://b2b.northfinder.com/xml/partners/dispatcher.php?param=43&token=FILL_ME_NORTHFINDER_TOKEN",
          "method": "GET",
          "headers": {},
          "params": {},
          "auth": {
            "mode": "none"
          }
        }
      }
    }
  },
  "invoices": {
    "layout": "flat",
    "months_back_default": 6,
    "download": {
      "strategy": "northfinder-web",
      "web": {
        "base_url": "https://b2b.northfinder.com",
        "login": {
          "mode": "playwright",
          "login_url": "https://b2b.northfinder.com/sk/login",
          "username": "FILL_ME_B2B_EMAIL",
          "password": "FILL_ME_B2B_PASSWORD",
          "storage_state_path": "suppliers/northfinder/state/storage_state.json",
          "selectors": {
            "cookie_accept_text": "Súhlasím",
            "user": "input[type='email'], input[name='email']",
            "pass": "input[type='password']",
            "submit": "button[type='submit']"
          }
        },
        "invoices_list_url": "https://b2b.northfinder.com/invoice_list",
        "datatable_ajax_url": "https://b2b.northfinder.com/sk/module/asdata_dashboard/invoice_list",
        "invoice_detail_base": "https://b2b.northfinder.com/invoice_detail/",
        "download_pdf_template": "https://b2b.northfinder.com/sk/module/asdata_dashboard/download_invoice_pdf?invoice_id={invoice_id}"
      }
    }
  },
  "adapter_settings": {
    "product_code_prefix": "NF-",
    "currency": "EUR",
    "vat_rate": 23,
    "mapping": {
      "invoice_to_canon": {
        "SCM": "Kód",
        "TITLE": "Názov",
        "QTY": "Množstvo",
        "UNIT_PRICE_EX": "Jednotková cena v EUR",
        "EAN": "EAN"
      },
      "postprocess": {
        "unit_price_source": "ex",
        "product_code_prefix": "NF-"
      }
    }
  }
}
